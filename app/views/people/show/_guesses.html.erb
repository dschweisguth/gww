<strong><%=h @person.username %> has correctly guessed <%= pluralize @guesses.length, 'photo' %></strong><br/>

<% @posters.each do |entry| -%>
<%=  entry[1].length %> <%= singularize 'were', entry[1].length %> posted by <%= link_to_person entry[0] %>:
<%   entry[1].each_with_index do |guess, i| -%>
<%=    link_to i + 1, show_photo_path(guess.photo) %>
<%   end -%>
<br/>
<% end -%>
<br/>
